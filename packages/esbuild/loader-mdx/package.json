{
  "name": "@blog/esbuild-loader-mdx",
  "description": "blog's mdx file loader for esbuild",
  "version": "1.0.0",
  "author": "xiaoboost",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p .",
    "watch": "pnpm run build -- -w",
    "build:type": "tsc -p . --emitDeclarationOnly",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/blog"
  },
  "peerDependencies": {
    "@mdx-js/react": "^1.6.0",
    "react": "^16.13.1 || ^17.0.0"
  },
  "dependencies": {
    "@blog/utils": "workspace:*",
    "@mdx-js/mdx": "^2.1.1",
    "@types/mdast": "^3.0.10",
    "@xiao-ai/utils": "^1.6.0",
    "remark": "^14.0.2",
    "remark-mdx": "^2.1.1",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.2",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@blog/tsconfig": "workspace:*",
    "@types/blog": "workspace:*",
    "@types/react": "^18.0.7",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  }
}
