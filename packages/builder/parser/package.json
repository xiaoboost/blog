{
  "name": "@blog/parser",
  "description": "blog's mdx parser lib",
  "version": "1.0.0",
  "author": "xiaoboost",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "require": "./dist/index.js"
    },
    "./walk": {
      "import": "./src/walk.ts",
      "types": "./src/walk.ts",
      "require": "./dist/walk.js"
    }
  },
  "scripts": {
    "test": "mocha",
    "build": "tsc -p ./tsconfig.build.json",
    "watch": "npm run build -- -w",
    "lint": "eslint --quiet src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --quiet --fix src/**/*.{ts,tsx,js,jsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/blog"
  },
  "dependencies": {
    "@blog/node": "workspace:*",
    "@blog/shared": "workspace:*",
    "@blog/types": "workspace:*",
    "@mdx-js/mdx": "2.3.0",
    "prettier": "2.8.4",
    "remark-gfm": "3.0.1",
    "remark-mdx": "2.3.0",
    "remark-parse": "10.0.1",
    "remark-stringify": "10.0.2",
    "tslib": "2.5.0",
    "unified": "10.1.2",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@blog/test-toolkit": "workspace:*",
    "@types/node": "16.11.7",
    "@types/prettier": "2.7.2",
    "@xiao-ai/eslint-config": "1.0.1",
    "@xiao-ai/tsconfig": "1.0.1",
    "eslint": "8.34.0",
    "mocha": "10.2.0",
    "typescript": "4.9.5"
  }
}
