{
  "name": "@blog/builder-template",
  "description": "blog's builder of template",
  "version": "1.0.0",
  "author": "xiaoboost",
  "license": "MIT",
  "source": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "bin": {
    "build-template": "./bin/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p .",
    "watch": "pnpm run build -- -w",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/blog"
  },
  "dependencies": {
    "@blog/config": "workspace:*",
    "@blog/esbuild-loader-file": "workspace:*",
    "@blog/esbuild-loader-jss": "workspace:*",
    "@blog/esbuild-loader-script": "workspace:*",
    "@blog/utils": "workspace:*",
    "@xiao-ai/utils": "^1.6.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@blog/tsconfig": "workspace:*",
    "@types/blog": "workspace:*",
    "@types/node": "^17.0.27",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "typescript": "^4.6.3"
  }
}
