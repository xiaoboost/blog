{
  "name": "@blog/context",
  "description": "blog's context package",
  "version": "1.0.0",
  "author": "xiaoboost",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./src/*/index.ts",
      "types": "./src/*/index.ts",
      "require": "./dist/*/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "npm run build -- -w",
    "lint": "eslint --quiet src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --quiet --fix src/**/*.{ts,tsx,js,jsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/blog"
  },
  "dependencies": {
    "@blog/types": "workspace:*",
    "@xiao-ai/utils": "1.6.7",
    "jss": "10.10.0",
    "jss-preset-default": "10.10.0",
    "tapable": "2.2.1",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@types/node": "20.19.27",
    "@xiao-ai/eslint-config": "1.0.3",
    "@xiao-ai/tsconfig": "1.0.4",
    "eslint": "8.56.0",
    "typescript": "5.9.3"
  }
}
