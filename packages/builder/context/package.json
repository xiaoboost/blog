{
  "name": "@blog/context",
  "description": "blog's context package",
  "version": "1.0.0",
  "author": "xiaoboost",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    "*": {
      "require": "./dist/cjs",
      "import": "./dist/esm"
    },
    "runtime": {
      "require": "./dist/cjs/runtime",
      "import": "./dist/esm/runtime"
    },
    "web": {
      "require": "./dist/cjs/web",
      "import": "./dist/esm/web"
    }
  },
  "scripts": {
    "clear": "rimraf ./dist",
    "build:cjs": "tsc -p . --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc -p . --module ESNext --outDir dist/esm",
    "build": "npm run clear && npm run build:cjs && npm run build:esm",
    "watch": "npm run build:cjs -- -w",
    "lint": "eslint --quiet src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --quiet --fix src/**/*.{ts,tsx,js,jsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/blog"
  },
  "dependencies": {
    "@blog/types": "workspace:*",
    "@xiao-ai/utils": "1.6.4",
    "jss": "10.9.2",
    "jss-preset-default": "10.9.2",
    "tapable": "2.2.1",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@xiao-ai/eslint-config": "1.0.1",
    "@xiao-ai/tsconfig": "1.0.1",
    "eslint": "8.22.0",
    "rimraf": "3.0.2",
    "typescript": "4.9.3"
  }
}
