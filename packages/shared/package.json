{
  "name": "@blog/shared",
  "version": "1.0.0",
  "description": "blog shared lib",
  "exports": {
    "./*": {
      "require": "./dist/*/index.js",
      "import": "./src/*/index.ts",
      "types": "./src/*/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "pnpm run build -- -w",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "author": "xiaoboost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/panda-manager"
  },
  "devDependencies": {
    "@blog/tsconfig": "workspace:*",
    "@types/blog": "workspace:*",
    "@types/color": "^3.0.3",
    "@types/fontmin": "^0.9.0",
    "@types/node": "^17.0.31",
    "tslib": "^2.4.0",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@xiao-ai/utils": "^1.6.0",
    "color": "^4.2.3",
    "fontmin": "^0.9.9",
    "jss": "^10.9.0",
    "pinyin-pro": "^3.10.0"
  }
}
