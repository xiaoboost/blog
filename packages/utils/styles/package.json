{
  "name": "@blog/styles",
  "version": "1.0.0",
  "description": "blog styles lib",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "require": "./dist/index.js"
    },
    "./constant": {
      "import": "./src/constant.ts",
      "types": "./src/constant.ts",
      "require": "./dist/constant.js"
    }
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "npm run build -- -w",
    "lint": "eslint --quiet src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --quiet --fix src/**/*.{ts,tsx,js,jsx}"
  },
  "devDependencies": {
    "@types/color": "3.0.3",
    "@types/node": "16.11.7",
    "@xiao-ai/eslint-config": "1.0.1",
    "@xiao-ai/tsconfig": "1.0.1",
    "eslint": "8.31.0",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@blog/context": "workspace:*",
    "@xiao-ai/utils": "1.6.6",
    "color": "4.2.3",
    "tslib": "2.4.1"
  }
}
