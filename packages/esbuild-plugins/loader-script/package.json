{
  "name": "@blog/esbuild-loader-script",
  "description": "blog's script loader for esbuild",
  "version": "1.0.0",
  "author": "xiaoboost",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "watch": "tsc -p . -w",
    "build": "tsc -p .",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/blog"
  },
  "dependencies": {
    "@blog/esbuild-loader-jss": "workspace:*",
    "esbuild": "^0.12.15",
    "fs-extra": "^10.0.0",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@blog/tsconfig": "workspace:*",
    "@types/blog": "workspace:*",
    "@types/fs-extra": "^9.0.12",
    "@types/md5": "^2.3.1",
    "eslint": "^7.30.0",
    "jss": "^10.7.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
